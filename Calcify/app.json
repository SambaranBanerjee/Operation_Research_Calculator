{
  "expo": {
    "name": "Calcify",
    "slug": "Calcify",
    "icon": "./assets/images/Calcify_logo.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "calcify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sambaran20.Calcify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Calcify_logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sambaran20.Calcify"
    },
    "splash": {
      "image": "./assets/images/Calcify_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6F4FE"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "754dc576-4874-4437-b687-8247c322b652"
      }
    }
  }
}
